<?xml version="1.0" encoding="UTF-8" ?>
<actionSystem>
    <context id="home.MainMenu" prefix="iemgr." mode="find">
        <group id="ImportGroup">
            <rules>
                <rule id="core.OpenGroup" mode="insertBehind"/>
            </rules>
            <content>
                <action id="Import"/>
            </content>
        </group>

        <group id="ExportGroup">
            <rules>
                <rule id="iemgr.ImportGroup" mode="insertBehind"/>
            </rules>
            <content>
                <action id="Export"/>
            </content>
        </group>
    </context>
</actionSystem>